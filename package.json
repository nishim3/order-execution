{
  "name": "order-execution",
  "version": "1.0.0",
  "description": "Mock DEX Router System with Order Execution API",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "dev:api": "nodemon src/api/server.ts",
    "db:clear": "bash scripts/clear-db.sh",
    "db:stats": "bash scripts/clear-db.sh stats",
    "test:concurrent": "npx ts-node src/api/test-concurrent.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:routing": "jest tests/routing.test.ts",
    "test:queue": "jest tests/queue.test.ts",
    "test:websocket": "jest tests/websocket.test.ts",
    "api:dev": "nodemon --exec ts-node src/api/server.ts",
    "api:build": "tsc",
    "api:start": "node dist/api/server.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "setup": "./scripts/setup.sh"
  },
  "keywords": [
    "dex",
    "trading",
    "order-execution",
    "websocket"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^8.4.0",
    "@fastify/websocket": "^8.3.1",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "axios": "^1.10.0",
    "bullmq": "^4.12.0",
    "fastify": "^4.24.3",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "pino-pretty": "^13.0.0",
    "uuid": "^9.0.1",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.8.7",
    "@types/ws": "^8.18.1",
    "dotenv": "^16.5.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
